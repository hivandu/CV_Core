    "import numpy as np\n",
    "# 药物衰减序列\n",
    "g = np.array([1, 0.8, 0.6, 0.2, 0., 0., 0., 0.])\n",
    "# 服药序列\n",
    "x = np.array([1, 1, 1, 1, 0, 0, 0, 0])\n",
    "# 卷积操作\n",
    "y = np.convolve(x, g)\n",
    "# 只取前五天的结果，最后一天为继续验证\n",
    "y = y[:5]\n",
